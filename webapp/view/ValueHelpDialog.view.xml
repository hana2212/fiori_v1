<mvc:View height="100%"
 controllerName="fiori2.controller.ValueHelpDialog"
		  xmlns:mvc="sap.ui.core.mvc"
		  xmlns:form="sap.ui.layout.form"
		  xmlns:core="sap.ui.core"
		  xmlns="sap.m">

	<form:SimpleForm editable="true">
		<Label text="search for ID" labelFor="multiInput"/>
		<MultiInput
			id="multiInput"
			width="60%"
			valueHelpRequest=".onValueHelpRequested"
			suggestionRows="{
				path: '/EmployeeSet',
				length: 10
			}">
			<suggestionColumns>
				<Column>
					<Label text="ID"/>
				</Column>
				<Column>
					<Label text="Name"/>
				</Column>
			</suggestionColumns>
			<suggestionRows>
				<ColumnListItem>
					<Label text="{ID}" />
					<Label text="{NAME}" />
				</ColumnListItem>
			</suggestionRows>
		</MultiInput>
		
	</form:SimpleForm>
</mvc:View>
